{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cam_trigger (Debug, integrated terminal)",
      "type": "cppdbg",
      "request": "launch",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "program": "${command:cmake.launchTargetPath}",
      "args": ["--duration", "10000", "--outdir", "/mnt/ssd/clips", "--verbose"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}